{
  "name": "inertium",
  "version": "1.2.1",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation|redux-persist|native-base(-shoutem-theme)|native-base|react-native-router-flux))"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "web": "react-scripts start",
    "web-bundle": "react-scripts build",
    "predeploy": "npm run web-bundle",
    "deploy": "gh-pages -d build",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19001'"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.1",
    "@rematch/core": "^1.1.0",
    "@rematch/loading": "^1.1.3",
    "@rematch/persist": "^1.1.6",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "expo": "^35.0.0",
    "expo-cli": "^3.0.10",
    "expo-font": "^7.0.0",
    "firebase": "^5.9.2",
    "immutability-helper": "^3.0.1",
    "native-base": "^2.12.1",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-native": "0.59.8",
    "react-native-action-button": "^2.8.5",
    "react-native-calendar-strip": "git+https://github.com/narvidas/react-native-calendar-strip.git",
    "react-native-easy-toast": "^1.1.0",
    "react-native-fbads": "^4.2.2",
    "react-native-gesture-handler": "^1.0.7",
    "react-native-router-flux": "4.0.6",
    "react-native-sortable-list": "0.0.21",
    "react-native-sortable-listview": "^0.2.8",
    "react-native-swipe-gestures": "^1.0.2",
    "react-native-toaster": "^1.2.2",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.2.2",
    "react-timer-mixin": "^0.13.4",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-preset-expo": "^5.1.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.0.1",
    "jest-expo": "^32.0.0",
    "node-sass": "^4.11.0",
    "react-scripts": "2.1.8"
  }
}
